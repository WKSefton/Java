public class DeckTester { 
  
  public static void main(String[] args)
  {
    //create random 52 card deck
    Deck test = new Deck();
    
    //print out sorted deck
    System.out.println(test.toString());
    
    //shuffle cards
    test.shuffleCards();
    
    //print out shuffled deck
    System.out.println(test.toString());
    
    //remove one card from the deck
    test.getCard();
    System.out.println(test.toString()):
  } 
}